{
  "name": "moviematchproject2021",
  "version": "1.0.0",
  "description": "MERN movie match app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinjohnkiely/movie-match-2021.git"
  },
  "author": "Kevin John Kiely",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kevinjohnkiely/movie-match-2021/issues"
  },
  "homepage": "https://github.com/kevinjohnkiely/movie-match-2021#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.13.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.12"
  }
}
